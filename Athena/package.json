{
  "name": "athena",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:web": "expo export --platform web",
    "serve": "serve dist",
    "start:web": "npm run build:web && npm run serve",
    "init-db": "ts-node ./scripts/init-db.ts",
    "db:create": "ts-node ./scripts/create-db.ts",
    "db:test": "ts-node ./scripts/test-db.ts",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:setup": "./scripts/setup-db.sh",
    "analyze:deps": "madge --circular ./",
    "analyze:deps:graph": "madge --image dependency-graph.svg ./",
    "analyze:bundle": "npm run build:web && source-map-explorer dist/static/js/*.js",
    "analyze:bundle:interactive": "WEBPACK_BUNDLE_ANALYZER=true npm run build:web",
    "build:debug": "expo export --platform web --webpack-config webpack.config.debug.js",
    "test:production": "node ./scripts/test-production-build.js",
    "test:production:simple": "npm run build:web && npm run serve",
    "load-test": "cd load-tests && ./run-tests.sh",
    "load-test:analysis": "k6 run load-tests/analysis.js",
    "load-test:streaming": "k6 run load-tests/streaming-analysis.js",
    "load-test:failover": "k6 run load-tests/ai-failover.js",
    "load-test:smoke": "k6 run --stage '1m:10,1m:20,1m:10,1m:5,1m:0' load-tests/analysis.js",
    "load-test:stress": "k6 run --stage '5m:50,10m:200,10m:500,5m:100' load-tests/analysis.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/slider": "^4.5.7",
    "@react-native-picker/picker": "^2.9.0",
    "@react-navigation/bottom-tabs": "^7.1.5",
    "@react-navigation/native": "^7.1.9",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "dotenv": "^16.5.0",
    "expo": "~52.0.46",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~12.8.1",
    "expo-device": "^7.1.4",
    "expo-document-picker": "~11.10.1",
    "expo-file-system": "~16.0.9",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "ioredis": "^5.6.1",
    "openai": "^4.103.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-google-fonts": "^1.2.1",
    "react-icons": "^5.5.0",
    "react-native": "0.76.9",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/jest-when": "^3.5.5",
    "@types/node": "^22.15.24",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "jest-when": "^3.7.0",
    "madge": "^8.0.0",
    "react-test-renderer": "18.3.1",
    "serve": "^14.2.4",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.3.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "private": true
}
