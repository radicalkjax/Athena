# Athena v2 Environment Configuration
# Copy this file to .env and fill in your values

# AI Provider API Keys
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here
VITE_MISTRAL_API_KEY=your_mistral_api_key_here

# Optional: Custom API Endpoints
# VITE_OPENAI_BASE_URL=https://api.openai.com/v1
# VITE_ANTHROPIC_BASE_URL=https://api.anthropic.com/v1
# VITE_DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
# VITE_MISTRAL_BASE_URL=https://api.mistral.ai/v1

# Security Settings
VITE_ENABLE_ENCRYPTION=true
VITE_ENCRYPTION_KEY=generate_a_secure_32_char_key_here

# Analysis Settings
VITE_MAX_FILE_SIZE_MB=100
VITE_ANALYSIS_TIMEOUT_MS=300000
VITE_ENABLE_CACHE=true
VITE_CACHE_TTL_MS=3600000

# Performance Settings
VITE_ENABLE_VIRTUAL_SCROLLING=true
VITE_ENABLE_LAZY_LOADING=true
VITE_MAX_CONCURRENT_ANALYSES=3

# Development Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info